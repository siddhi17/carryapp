<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    android:id="@+id/parentPanel"
    tools:context="com.carryapp.Fragments.TransportFragment">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="30dp"
            android:textColor="@android:color/black"
            android:text="@string/que" />

        <LinearLayout
            android:id="@+id/linearLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView6"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="50dp"
            android:orientation="vertical"
            android:weightSum="1">


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <EditText
                    android:id="@+id/editTextFrom"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_alignTop="@+id/imageView8"
                    android:layout_marginEnd="30dp"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginStart="05dp"
                    android:layout_toEndOf="@+id/imageView8"
                    android:layout_toRightOf="@+id/imageView8"
                    android:background="@color/colorPrimary"
                    android:cursorVisible="false"
                    android:focusable="false"
                    android:hint="@string/start"
                    android:inputType="textMultiLine"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/from"
                    android:textColor="@android:color/black" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginTop="30dp"
                    app:srcCompat="@mipmap/ic_place_black_48dp" />

                <View
                    android:id="@+id/view"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignEnd="@+id/editTextFrom"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignRight="@+id/editTextFrom"
                    android:layout_below="@+id/editTextFrom"
                    android:layout_marginTop="15dp"
                    android:alpha="0.3"
                    android:background="@drawable/line"></View>


            </RelativeLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp">

                <View
                    android:id="@+id/view1"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignEnd="@+id/editTextTo"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignRight="@+id/editTextTo"
                    android:layout_below="@+id/editTextTo"
                    android:layout_marginTop="15dp"
                    android:alpha="0.3"
                    android:background="@drawable/line"></View>

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/to"
                    android:textColor="@android:color/black" />

                <EditText
                    android:id="@+id/editTextTo"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_alignTop="@+id/imageView12"
                    android:layout_marginEnd="30dp"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginStart="05dp"
                    android:layout_toEndOf="@+id/imageView12"
                    android:layout_toRightOf="@+id/imageView12"
                    android:background="@color/colorPrimary"
                    android:cursorVisible="false"
                    android:ems="10"
                    android:focusable="false"
                    android:hint="@string/arrival"
                    android:inputType="textMultiLine"
                    android:minHeight="25dp"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginTop="30dp"
                    app:srcCompat="@mipmap/ic_place_black_48dp" />
            </RelativeLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="15dp">

                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignEnd="@+id/editTextTime"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignRight="@+id/editTextTime"
                    android:layout_below="@+id/editTextDate"
                    android:layout_marginTop="15dp"
                    android:alpha="0.3"
                    android:background="@drawable/line"></View>

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dateTime"
                    android:textColor="@android:color/black" />


                <EditText
                    android:id="@+id/editTextDate"
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/textView10"
                    android:layout_marginTop="20dp"
                    android:layout_toEndOf="@+id/imageView15"
                    android:layout_toRightOf="@+id/imageView15"
                    android:layout_toLeftOf="@+id/imageView"
                    android:layout_toStartOf="@+id/imageView"
                    android:background="@color/colorPrimary"
                    android:cursorVisible="false"
                    android:layout_marginLeft="05dp"
                    android:layout_marginStart="05dp"
                    android:focusable="false"
                    android:hint="@string/leavingDate"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_above="@+id/view3"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    app:srcCompat="@mipmap/ic_today_black_48dp" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dateTime"
                    android:textColor="@android:color/black" />

                <EditText
                    android:id="@+id/editTextTime"
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:layout_above="@+id/view3"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="05dp"
                    android:layout_marginStart="05dp"
                    android:background="@color/colorPrimary"
                    android:cursorVisible="false"
                    android:focusable="false"
                    android:hint="@string/leavingTime"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_above="@+id/view3"
                    android:layout_toLeftOf="@+id/editTextTime"
                    android:layout_toStartOf="@+id/editTextTime"
                    app:srcCompat="@mipmap/ic_query_builder_black_48dp" />
            </RelativeLayout>
        </LinearLayout>

        <Button
            android:id="@+id/btnSearch"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="250dp"
            android:layout_height="35dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/linearLayout6"
            android:layout_marginTop="60dp"
            android:background="@drawable/rounded_button"
            android:text="@string/search"
            android:textAllCaps="false"
            android:textColor="@color/colorPrimary"
            android:textSize="12sp" />


    </RelativeLayout>

</FrameLayout>
